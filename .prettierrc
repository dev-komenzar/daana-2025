{
	"useTabs": true,
	"tabWidth": 2,
	"printWidth": 99999,
	"singleAttributePerLine": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"semi": false,
	"trailingComma": "all",
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"bracketSameLine": false,
	"endOfLine": "lf",
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte",
				"svelteIndentScriptAndStyle": false
			}
		}
	]
}
