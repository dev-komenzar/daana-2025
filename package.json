{
	"name": "daana-2025",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "husky",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "eslint --fix .",
		"lint:style": "stylelint --fix --ignore-path .gitignore '**/*.{css,svelte}'"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.1",
		"@sveltejs/adapter-auto": "^6.1.1",
		"@sveltejs/enhanced-img": "^0.9.2",
		"@sveltejs/kit": "^2.48.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^22.19.0",
		"baseline-browser-mapping": "^2.9.15",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-perfectionist": "^4.15.1",
		"eslint-plugin-svelte": "^3.13.0",
		"eslint-plugin-unicorn": "^61.0.2",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.6",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"stylelint": "^16.25.0",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-recess-order": "^7.4.0",
		"stylelint-config-standard": "^39.0.1",
		"svelte": "^5.43.5",
		"svelte-check": "^4.3.3",
		"svelte-meta-tags": "^4.5.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.3",
		"vite": "^7.2.2",
		"vitest": "^4.0.8"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"lint-staged": {
		"*.+(js|ts|svelte)": [
			"prettier --write",
			"eslint --fix"
		],
		"*.css": [
			"prettier --write",
			"stylelint --fix"
		],
		"*.+(json|md|html)": "prettier --write",
		"*.svelte": "svelte-check --tsconfig ./tsconfig.json"
	},
	"dependencies": {
		"consola": "^3.4.2",
		"ky": "^1.14.0",
		"motion": "^12.23.25",
		"valibot": "^1.1.0"
	}
}
